<?xml version="1.0"?>
<extension key="com.skvare.dbcfcheck" type="module">
  <file>dbcfcheck</file>
  <name>Limit Custom Fields for the Custom Groups</name>
  <description>Limit the number of custom fields per custom group to avoid
    database errors based on the size of the field and type of collation of the
    column of custom fields.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Sunil Pawar</author>
    <email>sunil@skvare.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/Skvare/com.skvare.dbcfcheck</url>
    <url desc="Documentation">https://github.com/Skvare/com.skvare.dbcfcheck</url>
    <url desc="Support">https://github.com/Skvare/com.skvare.dbcfcheck</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-06-01</releaseDate>
  <version>1.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Dbcfcheck</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
